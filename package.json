    "html-webpack-plugin": "^3.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "react-test-renderer": "^16.8.4",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
