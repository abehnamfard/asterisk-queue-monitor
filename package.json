{
  "name": "asterisk-queue-monitor",
  "version": "1.0.0",
  "description": "Asterisk queue monitor",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-server && npm run build-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "dev": "webpack-dev-server --open 'google chrome' --mode development --config webpack.config.js",
    "clean": "rimraf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boreevyuri/asterisk-queue-monitor.git"
  },
