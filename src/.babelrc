{
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": [
          "transform-async-to-generator",
          "transform-regenerator"
        ]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module:fast-async",
      {
        "spec": true
      }
    ]
  ]
}