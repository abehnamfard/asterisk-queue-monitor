  },
  "homepage": "https://github.com/boreevyuri/asterisk-queue-monitor#readme",
  "dependencies": {
    "asterisk-ami-client": "^1.1.5",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
