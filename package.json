{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src",
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boreevyuri/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Yuri Boreev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boreevyuri/react-boilerplate/issues"
  },
  "homepage": "https://github.com/boreevyuri/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "expect": "latest",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1"
  }
}
